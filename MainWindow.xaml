<Window x:Class="Projekt1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="LOGIN:" HorizontalAlignment="Left" Margin="124,203,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.106,0.538"/>
        <TextBox x:Name="txt_Login" HorizontalAlignment="Left" Height="23" Margin="191,206,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="288" TextChanged="TextBox_TextChanged"/>
        <Label Content="PASSWORD:&#xD;&#xA;" HorizontalAlignment="Left" Margin="96,234,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txt_Password" HorizontalAlignment="Left" Height="23" Margin="191,238,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="288" TextChanged="Txt_Password_TextChanged"/>
        <Button x:Name="btn_Login" Content="LOG ON" HorizontalAlignment="Left" Margin="530,223,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" IsEnabled="False"/>

    </Grid>
</Window>
